@model IEnumerable<TMDT.ViewModels.MenuLoaiVM>

<div class="col-lg-12">
    <div class="mb-3">
        <h4>Danh Mục</h4>
        <ul class="list-unstyled fruite-categorie">
            @foreach (var item in Model)
            {
                <li>
                    <div class="d-flex justify-content-between fruite-name">
                        <a href="#"><i class="fas fa-apple-alt me-2"></i>@item.TenLoai</a>
                        <span>(@item.SoLuong)</span>
                    </div>
                </li>
            }

        </ul>
    </div>
</div>